package logicalprogram;

public class NoWhiteSpace {

	public static void main(String[] args) 
	{
		String s= "Pu n e";
		char space= ' ';
		int count=0;
		
		for(int i=0;i<=s.length()-1;i++)
		{
			if(s.charAt(i)==space)
			{
				count++;
			}
			
		}
		System.out.println("given String is "+s);
		System.out.println("number of blank space are "+count);
		
	}

}
